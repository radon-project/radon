class System {
    fun __constructor__() {
        ns = {}
        pyapi("import platform; import sys; os_name = platform.system(); platform = sys.platform", ns)
        this.name = ns["os_name"]
        this.platform = ns["platform"]
        this.version = "1.0"
    }
    fun getInfo() {
        return "System: " + this.name + " " + this.version
    }
}
