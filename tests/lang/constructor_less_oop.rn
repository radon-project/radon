class Algorithm {
    # Without __constructor__ Radon will auto create one for you!
    # fun __constructor__(){}

    fun find(list, value) {
        "Linear search -> return index"
        for i = 0 to len(list) {
            if list[i] == value {
                return i
            }
        }
        return -1
    }
}

var algo = Algorithm() # init object
var list = [1, 2, 3, 4, 5]
print(algo.find(list, 3))
