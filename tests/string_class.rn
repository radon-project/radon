const s = String("This is a string")

const s_upper = s.upper()
assert s_upper == "THIS IS A STRING"

const s_lower = s.lower()
assert s_lower == "this is a string"

const s_capitalize = s.capitalize()
assert s_capitalize == "This is a string"

const s_title = s.title()
assert s_title == "This Is A String"

const s_swapcase = s.swapcase()
assert s_swapcase == "tHIS IS A STRING"

const s_len = s.length()
assert s_len == 16

const s_count = s.count("i")
assert s_count == 3

try {
    s.count() # is throwing Python exception !!
} catch as err {
    print(err)
}
