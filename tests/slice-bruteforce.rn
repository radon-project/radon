
fun test(l) {
    for start = -10 to 10 {
        for end = -10 to 10 {
            for step_ = -10 to 10 {
                try {
                    print(l[start:end:step_])
                } catch as e {
                    print("ERROR: " + str(e))
                }
            }
        }
    }
    for start = -10 to 10 {
        for end = -10 to 10 {
            try {
                print(l[start:end])
            } catch as e {
                print("ERROR: " + str(e))
            }
        }
    }
    for start = -10 to 10 {
        try {
            print(l[start:])
        } catch as e {
            print("ERROR: " + str(e))
        }
    }
    for end = -10 to 10 {
        try {
            print(l[:end])
        } catch as e {
            print("ERROR: " + str(e))
        }
    }
    for step_ = -10 to 10 {
        try {
            print(l[::step_])
        } catch as e {
            print("ERROR: " + str(e))
        }
    }
    for end = -10 to 10 {
        for step_ = -10 to 10 {
            try {
                print(l[:end:step_])
            } catch as e {
                print("ERROR: " + str(e))
            }
        }
    }
    
    print(l[:])
    print(l[::])
}

