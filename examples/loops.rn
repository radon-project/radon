# Loops examples

# For loop

for i = 0 to 10 {
    print(i)
}

for i = 0 to 10 step 2 {
    print(i)
}

for i in "my string" {
    print(i)
}

for i in [1, 2, 3, 4, 5] {
    print(i)
}

for i in [1, 2, 3, 4, 5] {
    if i == 3 {
        break
    }
    print(i)
}


# While loop
#!
while i < 10 {
    print(i)
    i = i + 1
}
